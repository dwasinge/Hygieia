#!/bin/sh

echo "Script directory contents"
ls -la /usr/local/s2i

echo "current directory contents"
pwd
ls -la

echo "deployments directory contents"
ls -la /deployments

echo "tmp directory contents"
ls -la /tmp

echo "tmp/src directory contents"
ls -la /tmp/src

mkdir -p /tmp/src/m2/repository/com/capitalone/dashboard/core/3.1.0-DW
cp /tmp/src/core-3.1.0-DW.jar /tmp/src/m2/repository/com/capitalone/dashboard/core/3.1.0-DW/core-3.1.0-DW.jar

echo "repo directory contents"
ls -la /tmp/src/m2/repository/com/capitalone/dashboard/core/3.1.0-DW

/usr/local/s2i/assemble
